{
  "name": "blog-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "postinstall": "run-p install:*",
      "install:client": "cd client && npm install",
      "install:server": "cd server && npm install",
    "start": "npm-run-all -p -l start:*",
      "start:server": "cd server && node server.js",
      "start:client": "cd client && yarn start"
  },
  "license": "MIT",
  "dependencies": {
    "npm-run-all": "4.1.5"
  }
}
